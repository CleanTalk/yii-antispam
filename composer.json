{
    "name": "cleantalk/yii-antispam",
    "description": "Anti-spam yii extension by CleanTalk with protection against spam bots and manual spam.",
    "keywords": ["yii", "extension", "antispam", "cleantalk"],
    "homepage": "https://github.com/CleanTalk/yii-antispam",
    "type": "yii-extension",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Rustam Gumerov",
            "email": "psrustik@yandex.ru"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "cleantalk/php-antispam": "dev-master",
        "yiisoft/yii": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "phpunit/php-invoker": "*",
        "phpunit/dbunit": ">=1.2",
        "phpunit/phpunit-selenium":"*"
    },
    "autoload": {
        "classmap": ["CleanTalkApi.php", "CleanTalkValidator.php"]
    }
}